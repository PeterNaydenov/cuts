{
  "name": "@peter.naydenov/cuts",
  "description": "SPA scene manager",
  "version": "2.0.0",
  "license": "MIT",
  "author": "Peter Naydenov",
  "main": "./src/main.js",
  "type": "module",
  "types": "./types/main.d.ts",
  "exports": {
    ".": {
      "import": "./src/main.js",
      "require": "./dist/cuts.cjs",
      "default": "./src/main.js"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./src/*": "./src/*"
  },
  "scripts": {
    "dev": "vite",
    "build": "rollup -c",
    "postbuild": "tsc -p tsconfig.json",
    "preview": "vite preview",
    "test": "vitest",
    "cover": "vitest run --coverage",
    "cypress": "cypress open --component --browser chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeterNaydenov/cuts.git"
  },
  "devDependencies": {
    "@peter.naydenov/visual-controller-for-vue3": "^2.1.7",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^25.0.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.15",
    "cypress": "^15.7.1",
    "rollup": "^4.53.3",
    "typescript": "^5.9.3", 
    "vite": "^7.2.7",
    "vitest": "^4.0.15",
    "vue": "^3.5.25"
  },
  "dependencies": {
    "@peter.naydenov/log": "^1.1.1",
    "@peter.naydenov/shortcuts": "^4.0.0",
    "ask-for-promise": "^3.1.0"
  },
  "keywords": [
    "script",
    "visual",
    "state",
    "manager"
  ]
}
