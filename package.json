    "c8": "^10.1.3",
    "chai": "^5.1.2",
    "cypress": "^13.16.1",
    "mocha": "^11.0.1",
    "rollup": "^4.28.1",
    "vite": "^6.0.3",
    "vue": "^3.5.13"
