{
  "name": "@peter.naydenov/cuts",
  "description": "SPA scene manager",
  "version": "1.2.7",
  "license": "MIT",
  "author": "Peter Naydenov",
  "main": "./src/main.js",
  "type": "module",
  "types": "./types/main.d.ts",
  "exports": {
    ".": {
      "import": "./src/main.js",
      "require": "./dist/cuts.cjs",
      "default": "./src/main.js"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./src/*": "./src/*"
  },
  "scripts": {
    "dev": "vite",
    "build": "rollup -c",
    "postbuild": "tsc -p tsconfig.json",
    "preview": "vite preview",
    "test": "mocha test-mocha/*.js",
    "cover": "c8 mocha test-mocha/*.js",
    "cypress": "cypress open --component --browser chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeterNaydenov/cuts.git"
  },
  "devDependencies": {
    "@peter.naydenov/visual-controller-for-vue3": "^2.1.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^22.13.14",
    "@vitejs/plugin-vue": "^5.2.3",
    "c8": "^10.1.3",
    "chai": "^5.2.0",
    "cypress": "^14.2.1",
    "mocha": "^11.1.0",
    "rollup": "^4.38.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.4",
    "vue": "^3.5.13"
  },
  "dependencies": {
    "@peter.naydenov/log": "^1.1.1",
    "@peter.naydenov/shortcuts": "^3.1.3",
    "ask-for-promise": "^3.0.1"
  },
  "c8": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "node_modules",
      "test"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "keywords": [
    "script",
    "visual",
    "state",
    "manager"
  ]
}
